{
    "manifest_version": 2,
    "name": "Website Links Extension",
    "version": "1.0",
    "permissions": ["activeTab", "storage", "tabs"],
    "browser_action": {
      "default_popup": "default_popup.html",
      "default_icon": {
        "48": "icons/icon.png"
      }
    },
    "icons": {
      "48": "icons/icon.png"
    },
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self';"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  