{
    "manifest_version": 2,
    "name": "Website Links Manager",
    "version": "1.0",
    "permissions": ["activeTab", "tabs"],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "images/icon48.png"
    },
    "icons": {
      "48": "images/icon48.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  